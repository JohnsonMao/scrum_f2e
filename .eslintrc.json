{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/jsx-runtime",
		"react-app"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-tabs": "off",
		"max-len": "off",
		"arrow-parens": "off",
		"indent": ["off", "tab"],
		"no-param-reassign": "off",
		"no-underscore-dangle": "off",
		"object-curly-newline": "off",
		"comma-dangle": ["error", "never"],
		"quotes": [2, "single", { "avoidEscape": true }]
	}
}
